<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :artist_name %><br>
  <%= f.text_field :artist_name %><br>
  <br>
  <%= f.label :title %><br>
  <%= f.text_field :title %><br>
  <br>
  <%= f.label :genre %><br>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %><br>
  
  <input name="song[note_ids][]" id = "song_notes_1"><br>
  <input name="song[note_ids][]" id = "song_notes_2"><br>

  <br>
  
  <%= f.submit %>
<% end %>


